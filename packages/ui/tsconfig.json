    "allowJs": true,
    "declaration": true,
    "noEmit": false,
    "incremental": true,
    "outDir": "dist",
    "declarationDir": "types",
    "tsBuildInfoFile": ".Cache/tsc.buildinfo.json",
  },
  "include": ["."],
  "exclude": ["dist", "build", ".Cache", "types", "node_modules"]
}
